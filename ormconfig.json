{
   "type": "postgres",
   "host": "localhost",
   "port": 5432,
   "username": process.env.DB_user,
   "password": process.env.DB_PASS,
   "database": "notes-up",
   "entities": [
      "src/domain/entity/*.ts"
   ],
   "migrations": [
      "src/domain/migration/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/domain/entity",
      "migrationsDir": "src/domain/migration"
   }
}